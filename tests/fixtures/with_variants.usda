#usda 1.0
(
    defaultPrim = "Model"
    metersPerUnit = 1
    upAxis = "Y"
)

def Xform "Model" (
    variants = {
        string LOD = "high"
    }
    prepend variantSets = "LOD"
)
{
    variantSet "LOD" = {
        "high" {
            def Cube "Geometry"
            {
                double size = 2.0
            }
        }
        "low" {
            def Cube "Geometry"
            {
                double size = 1.0
            }
        }
    }
}
